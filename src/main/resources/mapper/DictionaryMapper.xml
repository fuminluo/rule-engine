<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.rule.engine.mapper.DictionaryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.rule.engine.entity.Dictionary">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="name" property="name" />
        <result column="ele_code" property="eleCode" />
        <result column="level_no" property="levelNo" />
        <result column="sort_no" property="sortNo" />
        <result column="parent_id" property="parentId" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code, name, ele_code, level_no, sort_no, parent_id, structure, remark
    </sql>

</mapper>
